generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Problems {
  id  Int @id @default(autoincrement())
  question_stem String
  number_of_options Int
  options Json
  number_of_right_options Int
  right_options Json
}